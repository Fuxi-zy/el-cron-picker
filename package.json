  },
  "author": {
    "name": "fuxishi",
    "email": "zhangzjx1422@163.com",
    "url": "https://github.com/fuxishi"
  },
  "main": "./dist/el-cron-picker.umd.cjs",
  "module": "./dist/el-cron-picker.es.js",
  "types": "./dist/el-cron-picker.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "dependencies": {
    "croner": "^9.1.0",
    "element-plus": "^2.11.7",
